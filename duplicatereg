apiVersion: apps/v1
kind: Deployment
metadata:
  name: registration-deployment-lowcode-1
  namespace: lowcode
spec:
  selector:
    matchLabels:
      app: registration-lowcode-1
  replicas: 1
  template:
    metadata:
      labels:
        app: registration-lowcode-1
    spec:
      containers:
      - name: registration
        image: sathish8329/registration:1
        ports:
        - containerPort: 3000
---
apiVersion: v1
kind: Service
metadata:
  name: registration-service-lowcode-1
  namespace: lowcode
spec:
  selector:
    app: registration-lowcode-1
  ports:
    - protocol: TCP
      port: 80
      targetPort: 3000
  type: NodePort
